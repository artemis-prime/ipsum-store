{
  "name": "ipsum-store",
  "version": "1.0.0",
  "main": "web/dist/index.js",
  "private": true,
  "packageManager": "yarn@1.22.5",
  "scripts": {
    "build:web": "yarn build:shared && yarn workspace @ipsum-labs/dash-web build",
    "build:shared": "yarn build:types && yarn build:util",
    "build:types": "yarn workspace @ipsum-labs/dash-types build",
    "build:util": "yarn build:types && yarn workspace @ipsum-labs/dash-util build",
    "build:server": "yarn build:shared && yarn workspace @ipsum-labs/dash-cloud-functions build",
    "serve:server": "yarn build:functions && yarn workspace @ipsum-labs/dash-cloud-functions serve",
    "serve:web": "yarn build:shared && yarn workspace @ipsum-labs/dash-web start",
    "clean:common": "rm -rf node_modules",
    "clean:server": "yarn workspace @ipsum-labs/dash-cloud-functions clean",
    "clean:types": "yarn workspace @ipsum-labs/dash-types clean",
    "clean:util": "yarn workspace @ipsum-labs/dash-util clean",
    "clean:web": "yarn workspace @ipsum-labs/dash-web clean",
    "clean:all": "yarn clean:common && yarn clean:types && yarn clean:util && yarn clean:web"
  },

  "workspaces": [
    "packages/web-app",
    "packages/wfw",
    "packages/shared-types",
    "packages/shared-util"
  ],
  "repository": "git@github.com:artemis-prime/ipsum-store.git",
  "author": "artemis prime <artemisprimedev@gmail.com>",
  "license": "GPL3.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-config-prettier": "^8.3.0",
    "typescript": "^4.4.3"
  }
}
