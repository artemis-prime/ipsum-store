// https://www.carlrippon.com/creating-react-app-with-typescript-eslint-with-webpack5/
{
  "presets": [
    "@babel/react", 
    "@babel/typescript", 
    ["@babel/env", { "modules": false }]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "regenerator": true }],
    ["inline-react-svg", {
      "svgo": {
        "plugins": [
          {
            "name": "preset-default",
            "params": {
              "overrides": {
                "removeViewBox": false 
              } 
            }
          }
          
            //"removeViewBox"
          //}
        ]
      }
    }]
  ]
}

